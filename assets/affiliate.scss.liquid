@media screen and (max-width: 1200px) {

  #affiliates .affiliates-testimonials {
    margin: 85px 0px;
  }

  #affiliates .affiliates-testimonials- .aff-text {
    width: 50%;
    font-size: 18px;
    line-height: 26px;
  }

  #affiliates .affiliates-testimonials- .aff-text .attribution {
