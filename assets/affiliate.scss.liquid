/* AFFILIATES PAGE */

#affiliates .banner-area {
  background: url("https://cdn.shopify.com/s/files/1/0874/6120/t/15/assets/affiliate_banner.jpg");
  background-size: cover;
  width: 100%;
  max-height: 670px;
  padding-top: 34.5%;
  position: relative;
}

#affiliates .banner-area h1 {
  text-indent: -9999px;
}

#affiliates .apply-banner {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  max-height: 650px;  

}

#affiliates .apply-banner .pink-frame {
  background: #fcfaf7;
  opacity: .8;
  width: 60%;
  max-width: 780px;
  margin: auto;
  padding: 95px 60px;
  text-align: center;
  position: relative;
  z-index: 500;
}

#affiliates .apply-banner .pink-frame h2 {
  color: #222;
}




#affiliates .apply-banner .pink-button {
  margin-top: -30px;
  position: relative;
  z-index: 600;
  padding: 9px 30px;
  color: #FFF;
  text-transform: uppercase;
  letter-spacing: 0.1em;
  font-family: "Novel",sans-serif;
  font-size: 12px;
}

  
@media screen and (max-width: 900px) {
  #affiliates .apply-banner .pink-frame {
    width: 90%;
    padding: 20px;
  }

  #affiliates .apply-banner .pink-frame h2 {
    font-size: 12px;
    line-height: 18px;
  }

  #affiliates .apply-banner .pink-button {
    margin-top: -20px;
    position: relative;
    z-index: 600;
  }

  #affiliates .apply-banner .pink-button a {
    font-size: 12px !important;
    background: #000 !important;
    padding: 10px;
  }
}

#affiliates h2 {
  font-size: 28px;
  font-weight: 700;
  color: #222;
  line-height: 33px;
  text-transform: uppercase;
  letter-spacing: 0.1em !important;
  display: block;
  margin: auto;
  text-align: center;
  margin-bottom: 40px;
}

#affiliates .apply-banner .pink-frame h2 {
  margin-bottom: 0px;
}

@media screen and (max-width:1023px) {
  #affiliates h2 {
    font-size: 18px;
    line-height: 29px;
  }
}

#affiliates .content-area {
  max-width: 1140px;
  margin: auto;
  padding: 0;
}

#affiliates .intro-area {
  margin: 100px 40px 100px 40px;
}

#affiliates .intro-area h2 {
  max-width: 525px;
}


#affiliates .intro-area p {
  font-size: 18px;
  line-height: 29px;
  font-weight: 100;
  color: #363636;
  text-align: center;
  max-width: 920px;
  margin: 0px auto 0px auto;
}

@media screen and (max-width:1023px) {
  #affiliates .intro-area p {
    font-size: 16px;
    line-height: 25px;
  }
}

#affiliates .perks-area {
  background: #e7e4df;
  width: 100%;
  max-width: 950px;
  margin: auto;
  height: auto;
  background-size: cover;
  padding: 80px 20px;
}

#affiliates .perks-area p {
  display:list-item; 
  list-style: disc !important;
  font-size: 15px;
  line-height: 30px;
  color: #363636;
  padding: 10px 0px;
  margin: 0px 140px;
}

@media screen and (max-width:1023px) {
  #affiliates .perks-area p {
    font-size: 15px;
    line-height: 25px;
    margin: 0px 80px;
  }
}

@media screen and (max-width:600px) {
  #affiliates .perks-area p {
    font-size: 15px;
    line-height: 25px;
    margin: 0px 50px;
  }
}

#affiliates .apply-button  {
  margin: auto;
  text-align: center;
  margin-top: 50px;
  max-width: 90%;
  cursor: pointer;
  cursor: hand;      
}

#affiliates .apply-button a {
  background-color: #363636;
  color: #FFF;
  width: auto;
  display: inline-block;
  margin: auto;
  text-align: center;
  padding: 9px 30px;
  color: #FFF;
  text-transform: uppercase;
  letter-spacing: 0.1em;
  font-family: "Novel",sans-serif;
  font-size: 12px;
}

#affiliates .apply-button a:hover {
  background: #000;
}

#affiliates .images-area {
  background: url("https://cdn.shopify.com/s/files/1/0874/6120/t/22/assets/primally_pure_affiliates.jpg?14728287489705302364");
  background-size: cover;
  width: 100%;
  max-height: 596px;
  padding-top: 52.74%;
  text-indent: -9999px;
  margin: 85px 0px;
}


#affiliates .who {
  margin-top: -25px;
  margin: -25px 85px 75px 85px;
}

@media screen and (max-width:600px) { 
  #affiliates .who {
    margin: -25px 40px 0px 40px;
  }
}

#affiliates .who .who-box {
  width: 285px;
  height: 285px;
  background: #e7e4df;
  background-size: 285px;
  float: left;
  margin: 0px 11px;
}

#affiliates .who .who1 {
  background: url("{{ 'who1.jpg' | asset_url }}");
    background-size: 285px;
    }

#affiliates .who .who2 {
  background: url("{{ 'who2.jpg' | asset_url }}");
    background-size: 285px;
    }

#affiliates .who .who3 {
  background: url("{{ 'who3.jpg' | asset_url }}");
    background-size: 285px;
    }

#affiliates .who .who-box h3 {
  text-align: center;
  text-transform: uppercase;
  font-size: 27px;
  margin-top: 132px;
  letter-spacing: 0 !important;
}

#affiliates .who .who-box .lm {
  font-size: 16px;
  text-align: center;
  font-family: "BellMT";
  text-transform: uppercase;
  text-decoration: underline;
  margin-top: 80px;
}

#affiliates .who .who-box:hover {
  background: #e7e4df;
}

#affiliates .who .who-sign {
  float: left;
  line-height: 285px;
  font-size: 40px;
  color: #363636;
}

@media screen and (max-width: 1360px) {
  #affiliates .who .who-box {
    width: 28.5%;
  }

  #affiliates .who .who-box h3 {
    font-size: 20px;
  }

  #affiliates .who .who-box .lm {
    font-size: 14px;
  }
}

@media screen and (max-width: 1140px) {
  #affiliates .who .who-box {
    width: 100%;
    height: 120px !important;
    border: 1px solid #FFF;
    float: none !important;
    clear: both;
    background-size: cover;
    background-position: center center;
    margin: 0px;
    padding: 0px;
    background-size: cover;
  }

  #affiliates .who .who-box h3 {
    font-size: 20px;
    margin: 31px 0px 10px 0px;
  }

  #affiliates .who .who-box .lm {
    font-size: 14px;
    margin-top: 0px;
    padding-bottom: 30px;
  }

  #affiliates .who .who-sign {
    width: 100%;
    height: auto;
    float: none;
    text-align: center;
    line-height: 18px;
    margin: 10px 0px;
  }
}

#affiliates .who-text {
  display: none;
  width: 100%;
  max-width: 950px;
  margin: auto;
}

#affiliates .who-box a {
  display: block;
  color: #363636;
  border-top: 1px solid #FFF;
}
#affiliates .who-text p {
  font-size: 18px;
  line-height: 29px;
  padding: 75px 0px 0px 0px;
  text-align: center;
}

#affiliates .hashtag {
  font-size: 20px;
  text-align: center;
  margin-top: 25px;
  margin-bottom: 80px;
}

#affiliates .product-area {
  background: url("https://cdn.shopify.com/s/files/1/0874/6120/t/15/assets/primal-products.jpg?2488480812481640665");
  background-size: cover;
  width: 100%;
  max-height: 650px;
  padding-top: 37.14%;
  text-indent: -9999px;
}


#affiliates .affiliates-testimonials- {
  clear: both;
  padding: 75px 75px 0px 75px;
  max-width: 950px;
  margin: auto;
  position: relative;
}

#affiliates .aff-test {
  margin-bottom: 60px;
  clear: both;
  /*display: flex;*/
  align-items: center;
}

#affiliates .affiliates-testimonials- .aff-image {
  width: 50%;
}

#affiliates .affiliates-testimonials- .aff-image img {
  width: 100%;
}

#affiliates .affiliates-testimonials- .aff-text {
  width: 50%;
  font-size: 15px;
  line-height: 30.3px;
  align-items: center;
}


#affiliates .attribution {
  font-family: "BellMT";
  font-size: 15px;
  letter-spacing: 0em;
}

#affiliates .attribution a {
  color: #363636;
}

#affiliates .attribution a:hover {
